{
  "name": "mobx-reactor",
  "version": "0.3.0",
  "description": "Connect MobX data stores to functional stateless React components with flux-like async actions and unidirectional data flow.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint src/**/*.js",
    "test": "ava"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "src/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "keywords": [
    "MobX",
    "React"
  ],
  "author": "Alexander M. Sauer-Budge",
  "license": "MIT",
  "peerDependencies": {
    "mobx": ">=2.0.4",
    "react": ">=0.14.7"
  },
  "devDependencies": {
    "ava": ">=0.13.0",
    "babel-cli": "6.x",
    "babel-core": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-decorators-legacy": "1.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "core-decorators": "^0.12.0",
    "react-dom": ">=0.14.7"
  },
  "dependencies": {}
}
